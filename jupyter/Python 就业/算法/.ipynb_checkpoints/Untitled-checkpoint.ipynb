   "source": [
    "import sklearn\n",
    "from sklearn.neighbors import KNeighborsClassifier\n",
    "\n",
    "#获取数据\n",
    "\n",
    "x = [[0], [1], [2], [3]]\n",
    "y = [0, 0, 1, 1]\n",
    "\n",
    "#实例化训练模型\n",
    "estimator=KNeighborsClassifier(n_neighbors=2)\n",
    "#调用fit训练\n",
    "estimator.fit(x,y)\n",
    "\n",
    "#预测其他值\n",
    "ret=estimator.predict([[2]])\n",
    "print(ret)"
   ]
